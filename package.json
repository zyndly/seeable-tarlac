    "html-react-parser": "^3.0.3",
    "moment": "^2.29.4",
    "next": "^12.2.5",
    "next-themes": "^0.2.0",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.3.0",
    "react-icons": "^4.4.0",
    "react-multi-carousel": "^2.8.2",
    "react-tippy": "^1.4.0",
